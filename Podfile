# platform :ios, '9.0'

target 'SFHacks' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SFHacks

  # Map
  pod ‘GoogleMaps’
  pod 'GooglePlaces'

  # DB
  pod 'RealmSwift'

  # API Client
  pod 'Alamofire'
